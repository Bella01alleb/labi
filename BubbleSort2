

A = [64, 34, 25, 12, 22, 11, 90]



def BubbleSort(A):
        for i in range(len(A)):
            for j in range(len(A) - 1 - i):
                if A[j] > A[j + 1]:
                    a = A[j]
                    A[j] = A[j + 1]
                    A[j + 1] = a


print("Sorted array is:")
for i in range(len(A)):
 print("% d" % A[i])
